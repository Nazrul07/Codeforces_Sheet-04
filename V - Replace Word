#include<iostream>
using namespace std;
int main()
{
    string s;
    cin>>s;
    int l,i,k=0;
    l=s.size();
    for(i=0 ; i<l ; i++)
    {
        if(s[i]=='E' && s[i+1]=='G' && s[i+2]=='Y' && s[i+3]=='P' && s[i+4]=='T')
        {
            cout<<" ";
            i+=4;
        }
        else
        {
            cout<<s[i];
        }
    }
    return 0;
}
